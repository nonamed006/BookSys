<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		 <typeAlias type="com.example.demo.vo.User" alias="user" />
		 <typeAlias type="com.example.demo.vo.Book" alias="book" />
		 <typeAlias type="com.example.demo.vo.Rent" alias="rent" />
		 <typeAlias type="com.example.demo.vo.Reply" alias="reply" />
		 
		 <typeAlias type="com.example.demo.dto.RentDto" alias="rentDto" />
		 <typeAlias type="com.example.demo.dto.RentUserDto" alias="rentUserDto" />
		 <typeAlias type="com.example.demo.dto.RentMailDto" alias="rentMailDto" />
		 <typeAlias type="com.example.demo.dto.ReplyDto" alias="replyDto" />
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/mappers/book.xml" /> 
		<mapper resource="mybatis/mappers/user.xml" /> 
		<mapper resource="mybatis/mappers/rent.xml" /> 
		<mapper resource="mybatis/mappers/reply.xml" /> 
	</mappers>
</configuration>